{
	"compilerOptions": {
	  "strict": true,
	  "module": "commonjs",
	  "moduleResolution": "node",
	  "target": "es2019",
	  "typeRoots": ["./types", "./node_modules/@types"],
	  "lib": ["esnext", "dom", "es2019", "es2020", "es2022.error"],
	  "removeComments": true,
	  "useUnknownInCatchVariables": false,
	  "forceConsistentCasingInFileNames": true,
	  "noImplicitAny": true,
	  "noImplicitReturns": true,
	  "noUnusedLocals": true,
	  "strictNullChecks": true,
	  "preserveConstEnums": true,
	  "resolveJsonModule": true,
	  "incremental": true,
	  "declaration": true,
	  "sourceMap": true,
	  "skipLibCheck": true,
	  "esModuleInterop": true,
	  "outDir": "./dist/",
	  "rootDir": "./"
	},
	"include": [
	  "./**/*",
	  "src/**/*",
	  "types/**/*",
	  "credentials/**/*",
	  "nodes/**/*",
	  "nodes/nxrender/**/*",
	  "nodes/**/*.json",
	  "package.json",
	  "nodes/nxrender/Nxrender.node.d.ts",
	  "nodes/nxrender/Nxrender.node.ts",
	  "credentials/NxrenderCredentialsApi.credentials.d.ts",
	  "credentials/NxrenderCredentialsApi.credentials.ts"
	]
  }
  